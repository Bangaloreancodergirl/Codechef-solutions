#include <stdio.h>
int main(void) {
	int t;
	scanf("%i",&t);
	while(t--){
        int n,c=0;
        scanf("%i",&n);
        while(n>0){
            if(n>=100){
                c++;
                n=n - 100;
            }
            else if(n>=50){
                c++;
                n=n - 50;
            }else if(n>=10){
                c++;
                n=n - 10;
            }else if(n>=5){
                c++;
                n=n - 5;
            }else if(n>=2){
                c++;
                n=n - 2;
            }else if(n>=1){
                c++;
                n=n - 1;
            }
        }
        printf("%d\n",c);
    }
	return 0;
}
